        // Material Kit CSS
        link(href="assets/css/material-dashboard.css?v=2.1.0" rel="stylesheet")
    body.body-toggle.dark-edition
        div.wrapper
            div.sidebar(data-color="danger" data-background-color="black")
                div.logo.pb-0(style="background-color:aquamarine")
                    a(href="http://me.swu.ac.id" class="simple-text logo-normal")
                        img(src="assets/img/myswu.png" alt="my-swu" width="50%")
                div.sidebar-wrapper
                    ul.nav
                        li.nav-item.active
                            a.nav-link(href="index.html")
                                i.material-icons school
                                p Dashboard Mahasiswa
                        li.nav-item
                            a.nav-link(href="jadwal.html")
                                i.material-icons event_note
                                p Jadwal Kuliah & Ujian
                        // your sidebar here
                        li.nav-item
                            a.nav-link(href="./assignment.html")
                                i.material-icons diversity_3
                                p Kuliah & Ujian
                        li.nav-item
                            a.nav-link(href="javascript:void(0)")
                                i.material-icons fact_check
                                p Nilai Ujian
                        li.nav-item
                            a.nav-link(href="javascript:void(0)")
                                i.material-icons checklist
                                p Peminatan Mata Kuliah
                        li.nav-item
                            a.nav-link(href="javascript:void(0)")
                                i.material-icons manage_accounts
                                p Pengaturan Akun
            div.main-panel
                // Navbar
                nav.navbar.navbar-expand-lg.navbar-transparent.navbar-absolute.fixed-top
                    div.container-fluid
                        div.navbar-wrapper
                            a.navbar-brand(href="javascript:void(0)") Dashboard Mahasiswa
                        button.navbar-toggler.justify-content-end(type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation")
                            span.sr-only Toggle navigation
                            span.navbar-toggler-icon.icon-bar
                            span.navbar-toggler-icon.icon-bar
                            span.navbar-toggler-icon.icon-bar
                // End Navbar
                div.content
                    div.container-fluid
                        // your content here
                        block content
                footer.footer
                    div.container-fluid
                        div.float-right.copyright
                            | &copy;
                            script.
                                document.write(new Date().getFullYear())
                            a(href="https://swu.ac.id" target="_blank") STMIK Widya Utama
                        //- // your footer here

